entity Affiliate {
	isActive Boolean
    notifyUser Boolean
    promoCode String
    promoCodeUrl String
    commissionPercentage Float
    commissionDate LocalDate
    createdDate LocalDate
    updatedDate LocalDate
}

entity Company {
	
}
entity Franchise {
}
entity PaymentMethod {
}

entity ElasticSearchAffiliate {
}

entity ElasticStatus{
}
entity Currency {

}
entity AffiliateCommissionReport {
	cutOffDate LocalDate
    createdDate LocalDate
    totalAmount Float
    notifyUser Boolean
}
entity ShippingOrder {

}
relationship OneToOne {
	ElasticSearchAffiliate to Affiliate
	
}

relationship OneToMany {
	Affiliate{affiliatedCustomers} to Company
    AffiliateCommissionReport{shipments} to ShippingOrder
}

relationship ManyToOne {
	ElasticSearchAffiliate{status} to ElasticStatus
    Affiliate{paymentMethod} to PaymentMethod
    Affiliate{affiliate} to Company
    Affiliate{franchise} to Franchise
    AffiliateCommissionReport{affiliate} to Affiliate
    AffiliateCommissionReport{currency}  to Currency
	
}
paginate all with pagination

dto all with mapstruct

service all with serviceImpl

