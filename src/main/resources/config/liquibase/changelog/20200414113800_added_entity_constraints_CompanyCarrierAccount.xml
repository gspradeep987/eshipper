<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">
    <!--
        Added the constraints for entity CompanyCarrierAccount.
    -->
    <changeSet id="20200414113800-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="region_id"
                                 baseTableName="company_carrier_account"
                                 constraintName="fk_company_carrier_account_region_id"
                                 referencedColumnNames="id"
                                 referencedTableName="country"/>

        <addForeignKeyConstraint baseColumnNames="carrier_id"
                                 baseTableName="company_carrier_account"
                                 constraintName="fk_company_carrier_account_carrier_id"
                                 referencedColumnNames="id"
                                 referencedTableName="carrier"/>

        <addForeignKeyConstraint baseColumnNames="franchise_id"
                                 baseTableName="company_carrier_account"
                                 constraintName="fk_company_carrier_account_franchise_id"
                                 referencedColumnNames="id"
                                 referencedTableName="franchise"/>

    </changeSet>
</databaseChangeLog>
