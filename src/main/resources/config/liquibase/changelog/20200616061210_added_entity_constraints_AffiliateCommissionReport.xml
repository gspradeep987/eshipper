<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity AffiliateCommissionReport.
    -->
    <changeSet id="20200616061210-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="affiliate_id"
                                 baseTableName="affiliate_commission_report"
                                 constraintName="fk_affiliate_commission_report_affiliate_id"
                                 referencedColumnNames="id"
                                 referencedTableName="affiliate"/>

        <addForeignKeyConstraint baseColumnNames="currency_id"
                                 baseTableName="affiliate_commission_report"
                                 constraintName="fk_affiliate_commission_report_currency_id"
                                 referencedColumnNames="id"
                                 referencedTableName="currency"/>

        <addForeignKeyConstraint baseColumnNames="status_id"
                                 baseTableName="affiliate_commission_report"
                                 constraintName="fk_affiliate_commission_report_status_id"
                                 referencedColumnNames="id"
                                 referencedTableName="commission_report_status"/>

    </changeSet>
</databaseChangeLog>
